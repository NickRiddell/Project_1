<%= form_for @show do |f| %>

<div>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :image %>
  <%= f.text_field :image%>
</div>
<div>
  <%= f.label :description %><br>
  <%= f.text_area :description %>
</div>
<div>
  <%= f.label :venue_id %><br>
  <%= f.select :venue_id, Venue.all.map {|v| [v.name, v.id] } %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>
