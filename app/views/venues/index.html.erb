<%= form_tag(venues_path, :method => "get", class: "navbar_form", id: "search-form") do %>
  <div class="input-append">
    <%= text_field_tag :search, params[:search], class: "span2", placeholder: "Search Venues" %>
    <button class="btn" type="submit"><i class="icon-search"></i></button>
  </div>
<% end %>
<% @venues.each do |venue| %>
<%= link_to("#{venue.name}", venue_path(venue)) %>
<% end %>
<br><br>
<% if can? :create, Venue %> 
<div>
<%=button_to('New Venue', new_venue_path, method: :get)%>
</div>
<% end %>